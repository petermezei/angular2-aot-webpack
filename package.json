{
  "name": "angular2-aot-webpack",
  "version": "1.0.0",
  "description": "Angular 2 AOT (Ahead Of Time) offline compilation example with Webpack",
  "main": "index.js",
  "scripts": {
    "ngc": "ngc -p ./tsconfig.aot.json",
    "ngc:watch": "node ./config/ngc-watch.js",
    "serve": "http-server ./build -a 0.0.0.0 -p 3000",
    "express": "node server",
    "build:developer": "npm run ngc && concurrently -r \"webpack-dev-server\" \"npm run ngc:watch\" \"gulp watch\" ",
    "build:developer:fast": "concurrently -r \"webpack-dev-server\" \"gulp watch\" ",
    "build:aot": "npm run ngc && npm run webpack:aot",
    "build:production": "npm run ngc && npm run webpack:production && npm run serve",
    "deploy": "gh-pages -d dist",
    "webpack": "webpack --config webpack.config.js",
    "webpack:aot": "webpack --config webpack.config.aot.js",
    "webpack:production": "webpack --config webpack.config.production.js"
  },
  "keywords": [
    "angular2",
    "aot",
    "typescript",
    "slamby",
    "website",
    "slamby.com"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slamby/slamby-website.git"
  },
  "author": {
    "name": "petermezei",
    "email": "peter.mezei@slamby.com"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/router": "^3.1.2",
    "body-parser": "^1.15.2",
    "bootstrap-sass": "^3.3.7",
    "connect": "^3.5.0",
    "core-js": "2.4.1",
    "express": "^4.14.0",
    "glob": "^7.1.1",
    "http": "0.0.0",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.2.1",
    "@angular/platform-server": "2.2.1",
    "@ngtools/webpack": "1.1.7",
    "@types/core-js": "0.9.34",
    "@types/node": "6.0.48",
    "angular2-template-loader": "0.6.0",
    "awesome-typescript-loader": "2.2.4",
    "browser-sync": "^2.18.2",
    "browser-sync-webpack-plugin": "^1.1.3",
    "chokidar": "1.6.1",
    "concurrently": "3.1.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "gh-pages": "0.12.0",
    "gulp": "^3.9.1",
    "gulp-pug": "^3.2.0",
    "gulp-sass": "^2.3.2",
    "html-loader": "^0.4.4",
    "http-server": "0.9.0",
    "image-webpack-loader": "^3.0.0",
    "imagemin-optipng": "^5.2.1",
    "node-sass": "^3.13.0",
    "pug-html-loader": "^1.0.9",
    "pug-plugin-ng": "0.0.2",
    "raw-loader": "0.5.1",
    "sass-loader": "^4.0.2",
    "sass-resources-loader": "^1.1.0",
    "source-map-explorer": "1.3.3",
